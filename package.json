{
    "name": "vscode-rubycommentdoc",
    "displayName": "Ruby Comment Doc",
    "description": "Generates comments for Ruby files",
    "version": "1.0.1",
    "publisher": "matthieumu",
    "icon": "images/icon.png",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.rubyCommentDoc"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.rubyCommentDoc",
                "title": "Ruby Comment Doc"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.11.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    },
    "bugs": {
        "url": "https://github.com/MatthieuMu/vscode-rubycommentdoc/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MatthieuMu/vscode-rubycommentdoc"
    }
}
